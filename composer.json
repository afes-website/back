{
    "name": "afes-website/back",
    "description": "afes.info's back-end",
    "keywords": ["festival"],
    "license": "proprietary",
    "type": "project",
    "version": "1.7.0",
    "homepage": "https://afes.info/",
    "readme": "./README.md",
    "time": "2020-04-11",
    "require": {
        "php": "^7.2",
        "ext-gd": "^7.2",
        "doctrine/dbal": "^2.10",
        "intervention/image": "^2.5",
        "laravel/lumen-framework": "^6.0",
        "lcobucci/jwt": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
